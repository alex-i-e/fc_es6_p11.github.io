    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "cross-env PORT=3100 node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "cross-env PORT=3101 node scripts/test.js --env=jsdom",
    "test-pure": "node scripts/test.js --coverage --env=jsdom",
    "test-snapshot-update": "npm run test-pure -- -u",
    "flow": "node_modules/.bin/flow",
    "server": "babel-node server.js --BROWSER=false",
    "hot-server": "nodemon server.js --exec babel-node --BROWSER=false"
  },
  "devDependencies": {
    "axios-mock-adapter": "1.16.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-flow": "6.23.0",
    "cross-env": "5.1.3",
    "eslint": "5.3.0",
    "eslint-config-react-app": "2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.11.0",
    "eslint-plugin-react-hooks": "0.0.0",
    "eslint-plugin-jest": "22.1.3",
    "flow-bin": "0.91.0",
    "istanbul": "0.4.5",
    "istanbul-api": "1.3.1",
    "istanbul-reports": "1.3.0",
    "jest": "22.4.4",
    "jsdom-global": "3.0.2",
    "prettier": "1.16.1",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.8.0-alpha.1",
    "redux-mock-store": "1.5.3"
  },
  "jest": {
    "verbose": true,
    "setupTestFrameworkScriptFile": "<rootDir>/src/tests/setupTests.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testRegex": ".*(spec)\\.(js|jsx)$",
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
