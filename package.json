    "eslint-plugin-tslint": "3.1.0",
    "eslint-plugin-typescript": "0.14.0",
    "istanbul": "0.4.5",
    "istanbul-api": "1.3.1",
    "istanbul-reports": "1.3.0",
    "jest": "22.4.4",
    "jsdom-global": "3.0.2",
    "prettier": "1.16.1",
    "react-addons-test-utils": "15.6.2",
